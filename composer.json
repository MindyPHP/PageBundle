{
    "name": "mindy/page-bundle",
    "license": "MIT",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0",
        "mindy/ckeditor-bundle": "~4.0",
        "mindy/form-bundle": ">=4.0.3",
        "mindy/mindy-bundle": "~4.0",
        "mindy/orm-bundle": "~4.0",
        "mindy/pagination-bundle": "~4.0",
        "mindy/seo-bundle": "~4.0",
        "mindy/sitemap-bundle": "~4.0",
        "mindy/template-bundle": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\PageBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Bundle\\PageBundle\\Tests\\": "./Tests/"
        }
    }
}
